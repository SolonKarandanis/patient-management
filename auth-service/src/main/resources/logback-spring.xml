<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <include resource="org/springframework/boot/logging/logback/base.xml"/>

    <appender name="CONSOLE_SQL" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%n%msg%n</pattern>
        </encoder>
    </appender>

    <logger name="net.ttddyy.dsproxy.listener.logging.SLF4JQueryLoggingListener" level="TRACE" additivity="false">
        <appender-ref ref="CONSOLE_SQL"/>
    </logger>

</configuration>